generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.db"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  senha String?
  nome  String?
  img   String?
}

model Preferencia {
  id                 Int     @id @default(autoincrement())
  areaDeInteresse1   String?
  areaDeInteresse2   String?
  areaDeInteresse3   String?
  areaDeInteresse4   String?
  nivelDeExperiencia String?
  idUsuario          String
}

model Pergunta {
  id               Int    @id @default(autoincrement())
  titulo           String
  descricao        String
  cursoRelacionado String
}
