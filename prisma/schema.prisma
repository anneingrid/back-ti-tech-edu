generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.db"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  nome  String?
  img   String?
}

model Pergunta {
  id    Int     @id @default(autoincrement())
  titulo String
  descricao String
  cursoRelacionado String
}